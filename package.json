{
  "name": "nwq",
  "version": "0.5.1",
  "description": "Base Node ES6 Project",
  "scripts": {
    "test": "node_modules/.bin/eslint test/*.js lib/*.js && mocha --compilers js:babel/register",
    "pretest": "echo '#!/bin/bash\nnpm test' >  .git/hooks/pre-commit && chmod +x  .git/hooks/pre-commit",
    "prepublish": "node_modules/.bin/browserify lib/advancer.js -t babelify > advancer.js && node_modules/.bin/browserify lib/SQSExchange.js -t babelify > SQSExchange.js && node_modules/.bin/browserify lib/MemoryExchange.js -t babelify > MemoryExchange.js"
  },
  "files": [ "advancer.js", "MemoryExchange.js", "SQSExchange.js" ],
  "repository": {
    "type": "git",
    "url": "https://github.com/forbesmyester/nwq.git"
  },
  "author": "@forbesmyester",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forbesmyester/nwq/issues"
  },
  "homepage": "https://github.com/forbesmyester/nwq",
  "dependencies": {
    "aws-sdk": "^2.2.3"
  },
  "devDependencies": {
    "babel": "^5.6.4",
    "babel-core": "^5.6.4",
    "babel-loader": "^5.1.4",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "chai": "^3.2.0",
    "eslint": "^0.24.0",
    "expect.js": "^0.3.1",
    "mocha": "^2.2.5"
  }
}
